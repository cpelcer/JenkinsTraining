job('simpleJobWithParameters'){
  
  steps {
      shell ('echo SUCCESS') 
  }  
}

job('generatedJob'){
  parameters {
   stringParam('Object', 'Undescribed', 'This parameter describes a type of a object') 
  }
  steps {
      shell ('echo $JOB_NAME $BUILD_NUMBER') 
      shell ('echo $OBJECT') 
  }
  
  publishers{
      downstream('simpleJobWithParameters', 'SUCCESS') 
  }
}
